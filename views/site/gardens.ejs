<%include ../layouts/header.ejs %> 
<!DOCTYPE html>
<html>
<head>
  <link href="/stylesheets/gardens.css" rel="stylesheet" type="text/css">
</head>
<body>
  <div class="container">
  <div class="row">
    <div class="col-xs-12">
    <div class="col-xs-18 col-sm-6 col-md-3">
     
  <% users.forEach(function (user){ %>
    
    <ul>
     <% user.gardens.forEach( function (garden) { %>
      <img src="<%= garden.title ? "/photos/" + garden.title : "" %>">
    <%})%>
    </ul>
  <% })%>
</div>
  </ul>
</div>
</div>

</div>
</body>
</html>